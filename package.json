{
  "name": "svelte-uikit-components",
  "version": "0.12.1",
  "license": "MIT",
  "author": {
    "name": "Wigtertainment Ltd",
    "email": "info@Wigtertainment.ltd",
    "url": "https://Wigtertainment.ltd"
  },
  "repository": {
    "url": "https://github.com/Wigtertainment/svelte-uikit-components"
  },
  "homepage": "https://svelte-uikit.wigtertainment.com",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "pretty": "prettier --write \"./**/*.{js,ts}\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.1",
    "uikit": "^3.16.15"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-netlify": "^2.0.7",
    "@sveltejs/kit": "^1.21.0",
    "@sveltejs/package": "^2.1.0",
    "prettier": "^2.8.8",
    "publint": "^0.1.13",
    "svelte": "^4.0.1",
    "svelte-check": "^3.4.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "prettier-plugin-svelte": "^2.10.1",
    "svelte-highlight": "^7.3.0"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "uikit",
    "components",
    "lib",
    "library",
    "typescript",
    "sveltekit"
  ]
}
