{
  "name": "svelte-uikit-components",
  "version": "0.16.0",
  "license": "MIT",
  "author": {
    "name": "Wigtertainment Ltd",
    "email": "info@Wigtertainment.ltd",
    "url": "https://Wigtertainment.ltd"
  },
  "repository": {
    "url": "https://github.com/Wigtertainment/svelte-uikit-components"
  },
  "homepage": "https://svelte-uikit.wigtertainment.com",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "pretty": "prettier --write \"./**/*.{js,ts}\""
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.2.1",
    "uikit": "^3.17.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.25.1",
    "@sveltejs/package": "^2.2.2",
    "prettier": "^3.0.3",
    "publint": "^0.2.3",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.10"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "prettier-plugin-svelte": "^3.0.3",
    "svelte-highlight": "^7.3.0"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "uikit",
    "components",
    "lib",
    "library",
    "typescript",
    "sveltekit"
  ]
}
