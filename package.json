{
	"name": "svelte-uikit-components",
	"version": "0.21.0",
	"license": "MIT",
	"author": {
		"name": "Wigtertainment Ltd",
		"email": "info@Wigtertainment.ltd",
		"url": "https://Wigtertainment.ltd"
	},
	"repository": {
		"url": "https://github.com/Wigtertainment/svelte-uikit-components"
	},
	"homepage": "https://svelte-uikit.wigtertainment.com",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"pretty": "prettier --write \"./**/*.{js,ts}\""
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.38.0",
		"uikit": "^3.23.11"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.2",
		"@sveltejs/adapter-netlify": "^5.1.1",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/package": "^2.4.1",
		"prettier": "^3.6.2",
		"publint": "^0.3.12",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"tslib": "^2.8.1",
		"typescript": "^5.2.2",
		"vite": "^7.1.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"prettier-plugin-svelte": "^3.4.0",
		"svelte-highlight": "^7.8.3"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"uikit",
		"components",
		"lib",
		"library",
		"typescript",
		"sveltekit"
	]
}
