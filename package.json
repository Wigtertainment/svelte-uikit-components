  "license": "MIT",
  "author": {
    "name": "Wigtertainment Ltd",
    "email": "info@Wigtertainment.ltd",
    "url": "https://Wigtertainment.ltd"
  },
  "repository": {
    "url": "https://github.com/Wigtertainment/svelte-uikit-components"
  },
  "homepage": "https://svelte-uikit.wigtertainment.com",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0",
    "uikit": "^3.16.15"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-netlify": "^2.0.7",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "gh-pages": "^5.0.0",
    "publint": "^0.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "svelte-highlight": "^7.3.0"
  }
}